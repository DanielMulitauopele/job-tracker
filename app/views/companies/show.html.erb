All Jobs for <strong><%= @company.name %></strong>

<% @company.jobs.each do |job| %>
  <ul><%= job.title %></ul>
<% end %>

<%= form_for(@company) do |f| %>

  <%= f.label :name %><br />
  <%= f.text_field :name %>

  <%= f.label :position %><br />
  <%= f.text_field :position %>

  <%= f.label :email %><br />
  <%= f.text_field :email %>


  <%= f.submit "Save" %>
<% end %>
