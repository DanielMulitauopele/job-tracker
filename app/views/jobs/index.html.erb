<h1>  All Jobs </h1>
<%= link_to 'Add a Job', new_company_job_path %><br>

<% @jobs.each do |job| %>
  <ul>
  <li><%= job.title %> </li>
  <li><%= job.company.name %></li>
  <li><%= job.city %></li>
  <li><%= job.level_of_interest %></li>

  <li><%= link_to "Edit", edit_job_path(job) %></li>
  <li><%= link_to "Delete", job_path(job), method: :delete %></li>
  </ul>
<% end %>
